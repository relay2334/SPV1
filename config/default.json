{
	"WebUI": {
		"title": "ExampleTitle",
		"slogan": "Example Slogan"
	},
	"Hotspot": {
		"ssid": "ShepardsPi",
		"key": "SHEPARDSPI"
	},
	
	"Database": {
		"uri": "mysql://sheep:sheep@localhost/sheep",
		"logging": false
	},
	"AppServer": {
		"port": 3000,
		"debug": false,
		"static": "static",
		"templates": "templates",
		"images": "images"
	},
	"NSFW": {
		"address": "http://localhost:4000"
	},
	"Monitoring": {
		"interface": "eth1",
		"Driftnet": {
			"autostart": true,
			"tmp": "/tmp/WallOfSheep/driftnet",
			"nsfw_filter": true
		},
		"TShark": {
			"autostart": true
		},
		"NGrep": {
			"autostart": true,
			"nsfw_filter": true
		},
		"Airmon": {
			"interface": "wlan0",
			"autostart": false,
			"interval": 5,
			"save_handshakes": true,
			"handshake_path": "/tmp/WallOfSheep/handshakes"
		},
		"Ettercap": {
			"autostart": true
		},
		"Nmap": {
			"autostart": true,
			"interval": 10,
			"target range": "192.168.1.0-100"
		}
	}
}
