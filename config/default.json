{
	"WebUI": {
		"title": "ExampleTitle",
		"slogan": "Example Slogan"
	},
	"Hotspot": {
		"ssid": "ShepardsPi",
		"key": "SHEPARDSPI"
	},
	"Airmon": {
		"interface": "wlan0",
		"airodump_autostart": true,
		"interval": 5,
		"save_handshakes": true,
		"handshake_path": "/tmp/WallOfSheep/handshakes"
	}
	"Database": {
		"uri": "mysql://sheep:sheep@localhost/sheep",
		"logging": false
	},
	"AppServer": {
		"port": 3000,
		"debug": false,
		"static": "static",
		"templates": "templates",
		"images": "images"
	},
	"NSFW": {
		"address": "http://localhost:4000"
	},
	"Monitoring": {
		"interface": "eth1",
		"Driftnet": {
			"autostart": true,
			"tmp": "/tmp/WallOfSheep/driftnet",
			"nsfw_filter": true
		},
		"TShark": {
			"autostart": true
		},
		"NGrep": {
			"autostart": true,
			"nsfw_filter": true
		},
		"Ettercap": {
			"autostart": true
		},
		"PVS": {
			"autostart": false,
			"hostname": "PVS_HOSTNAME",
			"username": "PVS_USERNAME",
			"password": "PVS_PASSWORD"
		}
	}
}
